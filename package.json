{
  "name": "gunbot-custom-trading-strategy-snippets",
  "displayName": "Gunbot Custom Trading Strategy Snippets",
  "publisher": "gunbot-trading-bot",
  "description": "Enhance your Gunbot custom trading strategies with ready-to-use JavaScript snippets. This extension provides quick access to Gunbot methods, data points, and technical indicators to streamline strategy development.",
  "version": "0.0.2",
  "icon": "img/logo192.png",
  "engines": {
    "vscode": "^1.55.0"
  },
  "main": "./out/extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/GuntharDeNiro/vscode-gunbot-custom-trading-strategy-snippets"
  },
  "homepage": "https://www.gunbot.com",
  "bugs": {
    "url": "https://github.com/GuntharDeNiro/vscode-gunbot-custom-trading-strategy-snippets/issues"
  },
  "author": {
    "name": "Gunbot",
    "email": "info@gunbot.com",
    "url": "https://www.gunbot.com"
  },
  "categories": [
    "Snippets",
    "Programming Languages"
  ],
  "keywords": [
    "Gunbot",
    "trading",
    "strategies",
    "indicators",
    "snippets",
    "automation",
    "JavaScript"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/snippets.code-snippets.json"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "snippetsViewContainer",
          "title": "Snippets",
          "icon": "img/logo192.svg"
        }
      ]
    },
    "views": {
      "snippetsViewContainer": [
        {
          "id": "snippetsView",
          "name": "Gunbot Custom Strategy"
        }
      ]
    }
  },
  "activationEvents": [
    "onLanguage:javascript"
  ],
  "dependencies": {
    "typescript": "^5.5.4"
  },
  "devDependencies": {
    "@types/node": "^22.5.3",
    "@types/vscode": "^1.55.0"
  }
}
